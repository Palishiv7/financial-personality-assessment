{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "src/css/**/*", "use": "@vercel/static" },
    { "src": "src/js/assessment.js", "use": "@vercel/static" },
    { "src": "src/js/biases.js", "use": "@vercel/static" },
    { "src": "src/js/financial-types.js", "use": "@vercel/static" },
    { "src": "src/js/main.js", "use": "@vercel/static" },
    { "src": "src/js/personality-display.js", "use": "@vercel/static" },
    { "src": "src/js/questions.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
} 